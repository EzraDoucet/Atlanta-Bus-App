<div class="container centered">
  <%= simple_form_for(@location) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :street_address %>
      <%= f.input :city %>
    </div>

    <div class="form-actions">
      <%= f.button :submit, class: 'btn btn-success'%>
    </div>
  <% end %>
</div>